# CPU
CONFIG_CPU_FREQ_STAT=y

# Fingerprint
CONFIG_FPR_FPC=n

# ION
CONFIG_ION_LEGACY=y

# OF
CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE=y

# Panic
CONFIG_PANIC_TIMEOUT=1

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_LZ4_COMPRESS=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Ramdisk
#CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y
CONFIG_RD_LZMA=y
CONFIG_RD_XZ=y

# Reduce size
CONFIG_SLUB_DEBUG=n
